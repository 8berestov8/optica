  "description": "A Strapi application",
  "scripts": {
    "push": "git add ./ && git commit -m 'auto' && git push",
    "strapi:develop": "cd packages/service && npm develop",
    "strapi:start": "cd packages/service && npm strapi start",
    "strapi:build": "cd packages/service && npm strapi build",
    "strapi": "cd packages/service && npm strapi",
    "postinstall": "cd packages/service && npm install && cd ../../packages/app && npm install",
    "start": "cd packages/app && npm start",
    "start:prod": "cd packages/app && npm start:prod",
    "build": "cd packages/app && npm run build",
    "build:prod": "cd packages/app && npm run build:prod",
    "android": "cd packages/app && npm android",
    "android:prod": "cd packages/app && npm android:prod"
  },
